 @model CadeteriaWeb.ViewModels.CadetesViewModels

<div>
    <h3>Subida de Cadetes</h3>

    <h5>Ingrese los datos del cadete</h5>
   


<form  method="post">
    <label class="form-label" asp-for="@Model.Nombre"></label>
    <input class="form-control" asp-for="@Model.Nombre" placeholder="Ingrese su Nombre del cadete" />
    <span asp-validation-for="@Model.Nombre" class="text-danger"></span>
    

    <label class="form-label" asp-for="@Model.Direccion"></label>
    <input class="form-control" asp-for="@Model.Direccion" placeholder="Ingrese la direccion del cadete"/>
    <span asp-validation-for="@Model.Direccion" class="text-danger"></span>
    

    <label class="form-label" asp-for="@Model.Telefono"></label>
    <input class="form-control" asp-for="@Model.Telefono" placeholder="Ingrese el telefono del cadete" />
    <span asp-validation-for="@Model.Telefono" class="text-danger"></span>
    

    <button  class="btn btn-primary">Enviar</button>
</form>

</div>

@section Scripts { 
                  <link href="~/css/EditorCadete.css" rel="stylesheet" asp-file-version="true" />
                  <partial name="_ValidationScriptsPartial" /> } 